{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "format": {
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start" : {
      "cache": false
    }
  }
}
